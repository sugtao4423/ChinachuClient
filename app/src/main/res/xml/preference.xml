<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="有効／無効">
        <CheckBoxPreference
            android:key="streaming"
            android:summaryOff="無効"
            android:summaryOn="有効"
            android:title="ストリーミング再生" />
        <CheckBoxPreference
            android:key="encStreaming"
            android:summaryOff="無効"
            android:summaryOn="有効"
            android:title="エンコ有りストリーミング再生" />
        <CheckBoxPreference
            android:key="oldCategoryColor"
            android:summaryOff="無効"
            android:summaryOn="有効"
            android:title="古いカテゴリー配色を使用" />
    </PreferenceCategory>

    <PreferenceCategory android:title="設定">
        <Preference
            android:key="addServer"
            android:title="サーバー追加" />
        <Preference
            android:key="settingActivity"
            android:title="サーバー設定変更" />
        <Preference
            android:key="delServer"
            android:title="サーバー削除" />
    </PreferenceCategory>

</androidx.preference.PreferenceScreen>